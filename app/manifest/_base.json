{
  "name": "Torus-mask",
  "short_name": "Torus-mask",
  "version": "0.0.1",
  "key": "igemgjllgkjldgfabdllfejnepdlagck",
  "manifest_version": 2,
  "author": "https://metamask.io",
  "description": "__MSG_appDescription__",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+M",
        "mac": "Alt+Shift+M",
        "chromeos": "Alt+Shift+M",
        "linux": "Alt+Shift+M"
      }
    }
  },
  "icons": {
    "16": "images/torus-icon-blue.png",
    "19": "images/torus-icon-blue.png",
    "32": "images/torus-icon-blue.png",
    "38": "images/torus-icon-blue.png",
    "64": "images/torus-icon-blue.png",
    "128": "images/torus-icon-blue.png",
    "512": "images/torus-icon-blue.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "bg-libs.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "images/torus-icon-blue.png",
      "19": "images/torus-icon-blue.png",
      "32": "images/torus-icon-blue.png",
      "38": "images/torus-icon-blue.png",
      "64": "images/torus-icon-blue.png",
      "128": "/images/torus-icon-blue.png",
      "512": "/images/torus-icon-blue.png"
    },
    "default_title": "Torus-mask",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "*://connect.trezor.io/*/popup.html"
      ],
      "js": [
        "vendor/trezor/content-script.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:8545/",
    "https://*.infura.io/",
    "activeTab",
    "webRequest",
    "*://*.eth/",
    "notifications"
  ],
  "web_accessible_resources": [
    "inpage.js",
    "phishing.html",
    "home.html"
  ],
  "externally_connectable": {
    "matches": [
      "https://metamask.io/*",
      "*://localhost/*",
      "https://google.com/*"
    ],
    "ids": [
      "*"
    ]
  }
}
